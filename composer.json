{
    "name": "icoverservices/dbal-fixtures",
    "version": "4.4.0",
    "description": "Fixtures management with Doctrine DBAL",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/dbal": "^4.0.x-dev",
        "symfony/console": "^6.4",
        "symfony/yaml": "^6.4",
        "fakerphp/faker": "^1.23.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.9",
        "phpspec/prophecy": "^1.18"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ComPHPPuebla\\Fixtures\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ComPHPPuebla\\Fixtures\\": [
                "tests/integration",
                "tests/src",
                "tests/unit"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test-with-stop": "./vendor/bin/phpunit --stop-on-failure --stop-on-error"
    }
}
