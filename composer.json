{
    "name": "comphppuebla/dbal-fixtures",
    "description": "Fixtures management with Doctrine DBAL",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/dbal": "^2.5 | ^3.3",
        "symfony/console": "^4.0 | ^5.0 | ^6.0",
        "symfony/yaml": "^4.0 | ^5.0 | ^6.0",
        "fakerphp/faker": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.9",
        "phpspec/prophecy": "^1.18"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ComPHPPuebla\\Fixtures\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ComPHPPuebla\\Fixtures\\": [
                "tests/integration",
                "tests/src",
                "tests/unit"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "chmod u+x bin/cli"
        ],
        "post-update-cmd": [
            "chmod u+x bin/cli"
        ],
        "test": "./vendor/phpunit/phpunit/phpunit",
        "test-with-stop": "./vendor/phpunit/phpunit/phpunit --stop-on-failure --stop-on-error"
    }
}
